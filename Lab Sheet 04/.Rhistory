find_outliers <- function(x) {
q1 <- quantile(x, 0.25)
q3 <- quantile(x, 0.75)
iqr <- q3 - q1
lower_fence <- q1 - 1.5 * iqr
upper_fence <- q3 + 1.5 * iqr
outliers <- x[x < lower_fence | x > upper_fence]
# Return result
if (length(outliers) == 0) {
return("No outliers")
} else {
return(outliers)
}
}
